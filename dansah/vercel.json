{
  "builds":[ {
    "src": "dansah/dansah/wsgi.py",
    "use": "@vercel/python",
    "config" : {
          "maxLambdaSize":  "15mb",
            "runtime":  "pyton3.9"}
  },
{
  "src": "build_files.sh",
  "use": "@vercel/static-build",
  "config": { "distDir": "staticfiles_build" }
}],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static_cdn/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dansah/dansah/wsgi.py"
    }
  ]

}